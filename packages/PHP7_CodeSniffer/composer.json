{
    "name": "symplify/php7_codesniffer",
    "description": "PHP7_CodeSniffer detects and fixes violations against your coding standard.",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "ext-tokenizer": "*",
        "ext-simplexml": "*",
        "squizlabs/php_codesniffer": "3.0.x-dev as 2.6",
        "symfony/console": "^3.2 as 3.1",
        "symfony/event-dispatcher": "^3.2",
        "symfony/finder": "^3.2",
        "symfony/options-resolver": "^3.2",
        "nette/bootstrap": "^2.4",
        "nette/di": "^2.4",
        "nette/robot-loader": "^2.4",
        "nette/utils": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "tracy/tracy": "^2.4",
        "slevomat/coding-standard": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\PHP7_CodeSniffer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\PHP7_CodeSniffer\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/php7cs"
    ]
}
