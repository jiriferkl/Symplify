{
    "name": "symplify/statie",
    "description": "Static Site Generator",
    "license": "MIT",
    "bin": [
        "bin/statie"
    ],
    "require": {
        "php": "^7.1",
        "erusev/parsedown-extra": "^0.7",
        "react/http": "^0.4",
        "ralouphie/mimey": "^1.0",
        "cpliakas/git-wrapper": "^1.7",
        "spatie/regex": "^1.1",

        "latte/latte": "^2.4",
        "zenify/modular-latte-filters": "^0.2",

        "nette/application": "^2.4",
        "nette/bootstrap": "^2.4",
        "nette/di": "^2.4",
        "nette/utils": "^2.4",
        "nette/finder": "^2.4",
        "tracy/tracy": "^2.4",

        "symfony/console": "^3.2",
        "symfony/translation": "^3.2",
        "symfony/yaml": "^3.2",

        "symplify/package-builder": "@dev"
    },
    "require-dev": {
        "nette/application": "^2.4",
        "phpunit/phpunit": "^5.7",
        "symplify/coding-standard": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\Statie\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\Statie\\Tests\\": "tests",
            "React\\Tests\\Http\\": "vendor/react/http/tests"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/symplify-cs check src tests",
        "fix-cs": "vendor/bin/symplify-cs fix src tests"
    }
}
